{
  "name": "trackflix",
  "version": "0.1.0",
  "description": "Trackflix React Client",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-scripts": "2.1.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
